<launch>
	<arg name="paused" default="false" />
	<arg name="use_sim_time" default="true" />
	<arg name="gui" default="true" />
	<arg name="headless" default="false" />
	<arg name="debug" default="false" />
    <arg name="world" default="p3dx" />
    <arg name="set_base_frame" default="base_link"/>
    <arg name="set_odom_frame" default="odom"/>
    <arg name="set_map_frame"  default="odom"/>
    <arg name="map_file" default="$(find mapless_navigation)/maps/ICM_map.yaml"/>
    <arg name="open_rviz" default="true"/>
    <arg name="move_forward_only" default="false"/>

    <node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>

    <!-- AMCL -->
    <include file="$(find robot_kist)/navigation/amcl.launch"/>
    <!--URDF robot-->
    <!--param name="robot_description" textfile="$(find myrobot)/urdf/mobile_robot_2.urdf"/-->
    <param name="robot_description"
            command="$(find xacro)/xacro --inorder '$(find p3dx_description)/urdf/pioneer3dx_Manh.xacro'" /> 
